/**app.wxss**/
page{
  min-height:100%;
  display: flex;
  flex-direction: column;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  background: #f4f4f4;
  flex: 1;
}
.color-ddd{
  color: #dddddd
}
.pd-10{
  padding:0 10rpx
}
.no-more{
  display: flex;
  flex: 1;
  align-items: center;
  align-self: center;
  width: 100%;
  text-align: center;
  margin: 0 auto;
}
.no-more .box image{
  width: 260rpx;
  height: 260rpx;
  margin: 0 auto;
}
.no-more .box{
  color: #999999;
  font-size: 30rpx;
  text-align: center;
  width: 100%;
  display: flex;
  flex-direction: column;
  letter-spacing: 4rpx;
}