.theme-background{
  background: #ffffff
}
.contact-us{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  box-sizing: border-box;
  background: #f4f4f4;
}
.contact-us-content{
  display: flex;
  flex-direction: column;
}
.content{
  padding: 30rpx 0;
  display: flex;
  flex-direction: column;
  color: #999999;
  font-size: 32rpx;
  text-align: center
}
.content text{
  line-height: 1.8;
}
.content .phone{
  color: #333333;
  font-size: 48rpx;
  font-weight: bold
}
.page-section{
  margin: 30rpx 0;
}